        <div class="container">
            <div class="row">
                <div class="cart-products col-lg-9 col-md-12 col-sm-12 col-xs-12">
                    <table>
                        <tr class="head-indicator">
                            <th class="image-indicator">
                            </th>
                            <th class="name-indicator">
                                <span>Product</span>
                            </th>
                            <th class="price-indicator">
                                <span>Price</span>
                            </th>
                            <th class="quantity-indicator">
                                <span>Quantity</span>
                            </th>
                            <th class="subtotal-indicator">
                                <span>Subtotal</span>
                            </th>
                        </tr>
                        <tr class="product-added">
                            <td class="product-image">
                                <img src="https://klbtheme.com/bacola/wp-content/uploads/2021/04/product-image-62-90x90.jpg" alt="">
                            </td>
                            <td class="product-name">
                                <span>All Natural Italian-Style Chicken Meatballs</span>
                            </td>
                            <td class="product-price">
                                $
